@model Task3.Models.MenuViewModel

<div>
    <nav class="navbar navbar-expand-lg bg-primary" data-bs-theme="dark">

        <ul class="navbar-nav me-auto">
            @foreach (var item in Model.MenuItems)
            {
                <li class="nav-item"><a href="@item.Url" class="nav-link">@item.Title</a></li>
            }
        </ul>

        <ul class="navbar-nav flex-grow-1 d-flex justify-content-end">
            <li class="nav-item">
                <a class="nav-link text-White fs-5 fw-bold" asp-area="" asp-controller="Security" asp-action="Logout">Logout</a>
            </li>
        </ul>
    </nav>
</div>
